  "theme_color": "#3BB9FF",
  "categories": [
    "productivity",
    "health",
    "business"
  ],
  "icons": [
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/dashboard-iphone.png",
      "sizes": "1284x2778",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "דשבורד יומי"
    },
    {
      "src": "/screenshots/clients-android.png",
      "sizes": "1080x2400",
      "type": "image/png",
      "form_factor": "wide",
      "label": "לקוחות ופגישות"
    }
  ],
  "shortcuts": [
    {
      "name": "פגישה חדשה",
