        {
          "src": "/icons/shortcut-appointments.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "לקוחות",
      "short_name": "לקוחות",
      "description": "רשימת לקוחות",
      "url": "/dashboard/clients",
