{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://luma-therapist-crm.vercel.app/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}